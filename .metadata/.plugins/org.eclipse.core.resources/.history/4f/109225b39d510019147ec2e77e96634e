
/** * Classe Point du plan avec ses cordonnées x et y */ 
public class Point extends Object{    
	private int x, y;    
	/**     
	 * * Methode qui affecte la valeur de son paramètre      
	 * * au Point this.     */        
	
	public void setX(int p) {        
		x = p;    
	}    
	
	public void setY(int p) {        
		y = p;    
	}    
	
	public int getX() {        
		return x;    
	}    
	
	public int getY() {        
		return y;    
	} 	
}
